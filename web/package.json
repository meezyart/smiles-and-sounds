        "markdown-it-anchor": "^8.1.0",
        "mocha": "^9.0.2",
        "node-fetch": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "phin": "^3.6.0",
        "postcss-cli": "^8.3.1",
        "pre-commit": "^1.2.2",
        "pre-push": "^0.1.1",
        "purge-from-html": "^1.0.3",
        "purgecss": "^4.0.3",
        "querystring": "^0.2.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.53.1",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.35.2",
        "sharp": "^0.28.3",
