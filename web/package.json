{
    "name": "sanity-eleventy-blog-web",
    "version": "1.0.7",
    "description": "",
    "main": "index.js",
    "scripts": {
        "clean-me": "rimraf dist",
        "js-build": "rollup -c rollup.config.js",
        "watch:js-build-watch": "rollup -c rollup.config.js -w",
        "watch:dev": "npx eleventy --watch --serve",
        "now-build": "npx eleventy && js-build",
        "debug": "DEBUG=* eleventy",
        "watch:sass": "sass  --no-source-map --watch src/scss:dist/assets/css",
        "build:sass": "sass  --no-source-map --style=compressed src/scss:dist/assets/css",
        "prefix": "postcss dist/assets/css/*.css -u autoprefixer -r",
        "dev": "cross-env ELEVENTY_ENV=development npm-run-all build:sass --parallel watch:*",
        "build": "cross-env ELEVENTY_ENV=production npm-run-all build:sass prefix now-build"
    },
    "keywords": [],
    "author": "",
    "pre-push": [
        "build"
    ],
    "pre-commit": [],
    "license": "ISC",
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-navigation": "^0.3.2",
        "@11ty/eleventy-plugin-rss": "^1.1.1",
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
        "@ampproject/toolbox-optimizer": "^2.8.3",
        "@babel/core": "^7.14.6",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@sanity/block-content-to-html": "^2.0.0",
        "@sanity/block-content-to-markdown": "0.0.5",
        "@sanity/client": "^2.11.0",
        "any-shell-escape": "^0.1.1",
        "autoprefixer": "^10.3.1",
        "babel-loader": "^8.2.2",
        "clean-css": "^5.1.3",
        "concurrently": "^6.2.0",
        "critical": "^4.0.0",
        "cross-env": "^7.0.3",
        "csp-hash-generator": "0.0.3",
        "csso": "^4.2.0",
        "datauri": "^3.0.0",
        "dotenv": "^10.0.0",
        "eleventy-plugin-local-images": "^0.4.1",
        "expect.js": "^0.3.1",
        "ffmpeg-static": "^4.4.0",
        "file-type": "^16.5.1",
        "fs-extra": "^10.0.0",
        "groq": "^2.2.6",
        "hasha": "^5.2.2",
        "html-minifier": "^4.0.0",
        "image-size": "^1.0.0",
        "jimp": "^0.16.1",
